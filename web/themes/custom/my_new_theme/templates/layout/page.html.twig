
{{ dump() }}


<div class="layout-container">

  <header role="banner">
    {{ page.header }}
  </header>

  {{ page.primary_menu }}

  {% if page.banner_top %}
    <div class="banner-top">
      {{ page.banner_top }}
    </div>
  {% endif %}

  <main role="main">
    <a id="main-content" tabindex="-1"></a>{# link is in html.html.twig #}

    <div class="layout-content">
      {{ page.content }}

      {{ page.content_bottom }}
    </div>
  </main>

  {% if page.banner_bottom %}
    <div class="banner-top">
      {{ page.banner_bottom }}
    </div>
  {% endif %}

  {% if page.footer %}
    <footer role="contentinfo">
      {{ page.footer }}
    </footer>
  {% endif %}

</div>{# /.layout-container #}
